{
  "manifest_version": 2,

  "name": "TL;DR",
  "description": "You should feel ashamed of being so lazy, but enjoy anyway.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
   "content_scripts": [ {
    "js": [ "jquery.min.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }],

  "permissions": ["tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}